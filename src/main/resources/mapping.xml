<?xml version="1.0" encoding="UTF-8"?>
<! - ORM is the root node of all mappings - >.
<orm>
<! - Use a mapping node for each pair of mappings - >
<mapping class="orm.Card" table="cards">
    <property name="id" column="card_id"/>
    <property name="name" column="card_name"/>
    <property name="value" column="value"/>
    <property name="superTypes" column="supertype"/>
    <property name="subTypes" column="subtype" />
    <property name="power" coulmn="c_power" />
    <property name="toughness" column="c_tough"/>
    <property name="description" column="c_desc"/>
    <property name="color" column="c_cost"/>
    <propery name="printSet" column="print_set"/>
</mapping>
</orm>